'''
Fizz buzz rules
if a number is divisible by 3, print fizz
if a number is divisible by 5, print buzz
if a number is divisible by 3 and 5, print fizzbuzz
'''

def fizz_buzz(n):
    if n % 3 == 0 and n % 5 == 0:
        return "fizzbuzz"
    elif n % 3 == 0:
        return "fizz"
    elif n % 5 == 0:
        return "buzz"
    else:
        return n

n = int(input("Enter a number: "))
print(fizz_buzz(n))

if n >= 7:
    if n <= 13:
        print("in range")
    else:
        print("out of range")
